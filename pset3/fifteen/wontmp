bool won(void)
{
    for (int i = 0; i < d; i++)
    {
        for (int j = 0; j < d; j++)
        {
            if (board[i][j] == board[i][j + 1] - 1)
            {
                return true;
            }
            else
            {
                return false;
            }
        }
    }
    return 0;
}
